<label for="geoblacklight_status">GeoBlacklight Status</label>
<select id="geoblacklight_status" name="geoblacklight_status" class="form-control">
    <option value="indexed">Index</option>
    <option value="notindexed">Do Not Index</option>
</select>
<label for="geoserver_style">Geoserver Default Style</label>
<select id="geoserver_style" name="geoserver_style" class="form-control">
    {{#each styles}}
    <option value="{{ this.name }}">{{ this.name }}</option>
    {{/each}}
</select>
<label for="mods_url">MODS URL</label>
<div class="form-group">
    <span id="mods_url">{{ data.mods_url }} </span>
</div>
<label for="mods_url">MODS Import URL</label>
<div class="form-group">
    <input type="checkbox" id="updateMods" />
    <label for="vehicle1">Check to update MODS xml</label><br>
    <input type="text" class="form-control" id="mods_import_url" value="{{ mod_import_url }}" />
</div>
<script>
    temp = jQuery.parseJSON("{{ dct_references_s }}".replace(/&quot;/g, '"'));
    console.log(temp);
    $('#mods_url').html('<a href="' + temp["http://www.loc.gov/mods/v3"] + '" target="_blank">' + temp["http://www.loc.gov/mods/v3"] + '</a>');
    $("#geoblacklight_status").val("{{status}}");
    $("#geoserver_style").val("{{style}}");
    $("#mods_import_url").val("{{mod_import_url}}");
</script>